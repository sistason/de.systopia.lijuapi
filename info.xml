<?xml version="1.0"?>
<extension key="de.systopia.lijuapi" type="module">
  <file>lijuapi</file>
  <name>Links Jugend API Connector</name>
  <description>Extension to connect to central LiJu Memebrship database and synchronize with CiviCRM</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Philipp Batroff</author>
    <email>batroff@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-01-07</releaseDate>
  <version>0.1</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Lijuapi</namespace>
    <angularModule>crmLijuapi</angularModule>
  </civix>
</extension>
